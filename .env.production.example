# JAGUAR FIGHT CLUB - Frontend Production Environment Template
# Copy this file to .env.production and fill in your production values

# API Configuration - Replace with your domain
VITE_API_URL=https://YOUR_DOMAIN.COM/api

# App Configuration
VITE_APP_NAME=JAGUAR FIGHT CLUB
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=production

# Domain Configuration - Replace with your domain
VITE_DOMAIN=YOUR_DOMAIN.COM

# Telegram Mini App Configuration - Replace with your bot details
VITE_TELEGRAM_BOT_NAME=your_production_bot_name
VITE_TELEGRAM_BOT_URL=https://t.me/your_production_bot_name

# Production Build Configuration
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true